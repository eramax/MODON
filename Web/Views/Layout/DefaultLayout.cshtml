    <script>
        var UppySetup = function (name, filesmaxcount = 1) {
            var uppy = Uppy.Core({
                autoProceed: true,
                restrictions: {
                    maxFileSize: 10485760,
                    maxNumberOfFiles: filesmaxcount,
                    allowedFileTypes: ['.JPEG', '.png', '.jpg', '.bmp', '.pdf', '.doc', '.docx', '.csv', 'xlsx', 'xls']
                }
            });
            uppy.use(Uppy.FileInput, {
                target: '.UppyInput-' + name,
                pretty: true,
                locale: {
                    strings: {
                        chooseFiles: 'اختر الملفات'
                    }
                }
            });
            uppy.use(Uppy.Tus, { endpoint: 'https://master.tus.io/files/' })
            uppy.use(Uppy.StatusBar, {
                target: '.UppyInput-Progress-' + name,
                hideUploadButton: true,
                hideAfterFinish: false,
                locale: {
                    strings: {
                        uploading: 'يتم رفع الملفات',
                        complete: 'تم رفع الملفات بنجاح'
                    }
                }
            });
            var filelist = "";
            uppy.on('complete', (result) => {
                result.successful.forEach(function (entry) {
                    filelist += entry.id + ",";
                });
                $("#" + name).attr('value', filelist);
            })
        }

        var LoadModal = function (url) {
            $.get(url, function (res) {
                $mymodal = $("#ModalId");
                $mymodal.find("div.modal-content").html(res);
                $mymodal.modal("show");
            });
        }
    </script>


